{
	"name": "rotoscodejs-vscodeext",
	"displayName": "RotoscodeJS Recorder",
	"description": "Capture code changes and share them using a player widget.",
	"version": "1.1.0",
	"publisher": "projectkenneth",
	"license": "SEE LICENSE IN LICENSE",
	"keywords": [
		"code recording",
		"snippet",
		"recording",
		"share",
		"player"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/projectkenneth/rotoscodejs-recorder"
	},
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:rotoscodejs-recorder.startRecording"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "rotoscodejs-recorder.startRecording",
				"title": "RotoscodeJS: Recording Panel"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
